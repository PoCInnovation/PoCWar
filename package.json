{
  "name": "pocwar",
  "version": "0.0.1",
  "private": true,
  "description": "pocwar repository",
  "homepage": "https://github.com/PoCFrance/PoCWar",
  "scripts": {
    "dev": "PATH_HOST=$(pwd) docker-compose up -d --build",
